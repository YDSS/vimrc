syntax on

set fencs=utf-8,gb18030,gb2312,latin-1
set fenc=utf-8
set enc=utf-8

set nu
set ts=4
set sw=4
set smarttab
set expandtab
set autoindent
set cindent
" set cursorline

set foldmethod=marker
set foldmarker={,}
set foldlevel=10


" NERDTree
map <F7> <Esc>:NERDTreeToggle<CR>
map <F8> <Esc>:TlistToggle<CR>

let NERDTreeWinSize=45